<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Kişisel Bilgi Gözetimi"
        android:id="@+id/textView12"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="42dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Family Safe çocuğunuz çevrimiçi iken hangi kişisel bilgiler ile giriş yaptığını size gösterir.İsterseniz Giriş bilgilerini Güncelleyebilirsiniz"
        android:id="@+id/textView13"
        android:layout_below="@+id/textView12"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="48dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Family Safe Kullanıcı Adı"
        android:id="@+id/textView17"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtFamilySafeKullanıcıAdı"
        android:width="170dp"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/textView12"
        android:layout_toEndOf="@+id/textView12"
        android:background="#ff86ff37" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Family Safe Mail Adresi"
        android:id="@+id/textView18"
        android:layout_below="@+id/txtFamilySafeKullanıcıAdı"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:visibility="invisible" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtFamilySafeMailAdress"
        android:width="170dp"
        android:background="#ff86ff37"
        android:layout_alignBottom="@+id/textView18"
        android:layout_alignLeft="@+id/txtFamilySafeKullanıcıAdı"
        android:layout_alignStart="@+id/txtFamilySafeKullanıcıAdı"
        android:visibility="invisible" />
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Family Safe Çocuk Adı"
    android:id="@+id/textView19"
        android:layout_below="@+id/textView18"
        android:layout_alignRight="@+id/textView18"
        android:layout_alignEnd="@+id/textView18" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtFamilySafeCocukAdı"
        android:width="170dp"
        android:layout_alignTop="@+id/textView19"
        android:layout_alignLeft="@+id/txtFamilySafeMailAdress"
        android:layout_alignStart="@+id/txtFamilySafeMailAdress"
        android:background="#ff86ff37" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Çocuk Cinsiyet"
        android:id="@+id/textView20"
        android:layout_below="@+id/txtFamilySafeKullanıcıAdı"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="90dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtFamilySafeCocukCinsiyet"
        android:width="170dp"
        android:background="#ff86ff37"
        android:layout_alignTop="@+id/textView20"
        android:layout_alignLeft="@+id/txtFamilySafeCocukAdı"
        android:layout_alignStart="@+id/txtFamilySafeCocukAdı" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Çocuk Doğum Tarihi"
        android:id="@+id/textView21"
        android:layout_below="@+id/txtFamilySafeKullanıcıAdı"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="140dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtFamilySafeCocukDogumTarihi"
        android:width="170dp"
        android:background="#ff86ff37"
        android:layout_alignBottom="@+id/textView21"
        android:layout_alignLeft="@+id/txtFamilySafeCocukCinsiyet"
        android:layout_alignStart="@+id/txtFamilySafeCocukCinsiyet" />

</RelativeLayout>
